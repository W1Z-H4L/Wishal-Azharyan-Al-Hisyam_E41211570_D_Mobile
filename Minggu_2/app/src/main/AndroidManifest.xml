<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Minggu_2"
        tools:targetApi="31">
        <activity
            android:name=".linearlayout"
            android:exported="false"
            android:label="Linear Layout"/>
        <activity
            android:name=".mainmenu"
            android:exported="true"
            android:label="Main Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".customize"
            android:exported="false"
            android:label="Login"/>
        <activity
            android:name=".materialdesign"
            android:exported="false"
            android:label="Material Design"/>
        <activity
            android:name=".framelayout"
            android:exported="false"
            android:label="Frame Layout"/>
        <activity
            android:name=".horizontallscroll"
            android:exported="false"
            android:label="Horizontal Scroll"/>
        <activity
            android:name=".relativelayout"
            android:exported="false"
            android:label="Relative Layout"/>
        <activity
            android:name=".verticalscroll"
            android:exported="false"
            android:label="Vertical Layout"/>
        <activity
            android:name=".tabellayout"
            android:exported="false"
            android:label="Tabel Layout"/>
    </application>

</manifest>